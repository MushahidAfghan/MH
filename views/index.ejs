<%-include('layout/header') %>

<div class="contaier">
  <div class="row mt-5">
    <div class="col-lg-12"></div>
    <h1>
        Display Users
    </h1>
    <div class="table-responsive">
      <table class="table table-striped">
        <thead>
          <tr class="table-dark">
            <th>ID</th>
            <th>Nam</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <ul>
            <% for (user of users) { %>
            <tr>
                <td><%= user._id %></td>
              <td><%= user.name %></td>
              <td><%= user.email %></td>
              <td><%= user.phone %></td>
              <td>
                <a href="/edit/<%=user._id %> ">Edit</a>
                <a href="/del/<%=user._id %> ">Del</a>
              </td>
            </tr>

            <% } %>
          </ul>
        </tbody>
      </table>
    </div>
  </div>
</div>

<%- include('layout/footer') %>
