<%-include('layout/header')  %> 

<div class="contaier">
    <div class="row">
        <div class="col-md-6 offset-md-3 " id="form">
            <h1>Insert Student</h1>
            <form autocomplete="off" action="/add" method="POST" enctype="multipart/form-data	">
                <div class="form-group ">
                    <label for=""> Name </label>
                    <input type="text" class="form-control" name="name" placeholder="Full Name">
                </div >
                <div class="form-group">
                    <label for="">Email</label>
                    <input type="email" class="form-control" name="email" placeholder="Email">
                </div>
                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label for="">Mobile</label>
                        <input type="tel" class="form-control" name="phone" placeholder="Mobile">
                    </div>
                   
                </div>
                <div class="form-group">
                    <button class="btn btn-info" type="submit"><i class="fa fa-database"></i> ADD User</button>
                </div>
            </form>
        </div>
       </div>
</div>


<%- include('layout/footer')  %> 